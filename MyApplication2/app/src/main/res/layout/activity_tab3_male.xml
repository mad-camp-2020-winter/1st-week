<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Tab3Male">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="33dp"
        android:text="이상형 월드컵 8강"
        android:id="@+id/male_round8"
        android:gravity="center"
        android:background="@color/cardview_shadow_start_color"
        android:textColor="@color/black"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="33dp"
        android:text="이상형 월드컵 4강"
        android:id="@+id/male_round4"
        android:gravity="center"
        android:background="@color/cardview_shadow_start_color"
        android:textColor="@color/black"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="33dp"
        android:text="이상형 월드컵 2강"
        android:id="@+id/male_round2"
        android:gravity="center"
        android:background="@color/cardview_shadow_start_color"
        android:textColor="@color/black"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="33dp"
        android:text="☆☆☆이상형 월드컵 결승전☆☆☆"
        android:id="@+id/male_round1"
        android:gravity="center"
        android:background="@color/cardview_shadow_start_color"
        android:textColor="@color/black"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="362dp"
            android:id="@+id/male_first_button"
            android:scaleType="centerCrop"
            android:layout_margin="1dp"
            android:background="#00000000"
            android:paddingRight="20dp"
            android:paddingBottom="20dp"
            android:layout_gravity="end|bottom"
            android:padding="0dp"></ImageButton>


        <ImageButton
            android:layout_width="match_parent"
            android:layout_height="362dp"
            android:id="@+id/male_second_button"
            android:scaleType="centerCrop"
            android:layout_margin="1dp"
            android:background="#00000000"
            android:paddingRight="20dp"
            android:paddingBottom="20dp"
            android:layout_gravity="end|bottom"
            android:padding="0dp"></ImageButton>


    </LinearLayout>

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_gravity="center"
        android:src="@drawable/vs"
        tools:ignore="MissingConstraints" />

</FrameLayout>